%Sample code
close all;
clear all;


img=imread('images/sample.png');

ima = img;
[x,y] = size(ima);
colormap(gray(256));
figure();
image(ima);
figure(2);
ima = binarize(ima);
ima = inverse_bin(ima);
ima = dil(buildStructuralElementForDim(7),ima);
lines = findLineBounds(ima,0.001,50);
disp(lines);
ima = back_inverse_bin(ima);
colormap(gray(256));
image(ima);

